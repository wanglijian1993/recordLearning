/**
 * Definition for singly-linked list.
 * public class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) { val = x; }
 * }
 */

public ListNode reverseList(ListNode head) {
     ListNode head=null; 
     ListNode curr=head; 
     while(curr!=null){
       ListNode temp=curr.val;
       curr=curr.next;
       head=curr.next;
       curr.next=head;
    }

}

